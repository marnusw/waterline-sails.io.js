{
  "name": "waterline-sails.io.js",
  "version": "0.3.1",
  "description": "sails.io.js adapter for Sails / Waterline when using Waterline on a client to communicate with a Sails server",
  "main": "index.js",
  "scripts": {
    "test": "node runner.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marnusw/waterline-sails.io.js.git"
  },
  "keywords": [
    "sails.io.js",
    "web sockets",
    "isomorphic",
    "adapter",
    "sails",
    "waterline",
    "orm",
    "sailsjs",
    "sails.js"
  ],
  "author": "Marnus Weststrate <marnusw@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "lodash": "~2.4.1",
    "sails.io.js": "~0.11.5",
    "socket.io-client": "~1.3.3",
    "waterline-errors": "~0.10.1"
  },
  "devDependencies": {
    "captains-log": "^0.11.11",
    "mocha": "^2.2.5",
    "sails": "^0.11.0",
    "sails-memory": "^0.10.5",
    "waterline-adapter-tests": "^0.10.13"
  },
  "waterlineAdapter": {
    "waterlineVersion": "~0.10.24",
    "interfaces": [
      "semantic",
      "queryable"
    ],
    "features": [
      "cross-adapter",
      "unique",
      "autoIncrement.sequential"
    ]
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/marnusw/waterline-sails.io.js/issues"
  },
  "homepage": "https://github.com/marnusw/waterline-sails.io.js"
}
